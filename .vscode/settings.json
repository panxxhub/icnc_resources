{
	"ros.distro": "humble",
	"editor.fontFamily": "'Cascadia Code', 'Fira Code Retina', 'Jetbrains Mono', Consolas, 'Courier New', monospace",
	"editor.fontLigatures": true,
	"editor.lightbulb.enabled": true,
	"editor.experimental.stickyScroll.enabled": true,
	"editor.experimental.pasteActions.enabled": true,
	"editor.experimental.useCustomCodeActionMenu": true,
	"editor.bracketPairColorization.enabled": true,
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.cursorSmoothCaretAnimation": true,
	"editor.codeLens": true,
	"editor.codeLensFontFamily": "'Fira Code Retina', 'Cascadia Code', 'Jetbrains Mono', Consolas, 'Courier New', monospace",
	"editor.cursorStyle": "line-thin",
	"editor.guides.highlightActiveBracketPair": true,
	"editor.colorDecorators": true,
	"editor.glyphMargin": true,
	"workbench.colorTheme": "Default Dark+",
	"C_Cpp.autocomplete": "Default",
	"C_Cpp.intelliSenseEngine": "Disabled",
	"clangd.path": "clangd-15",
	"clangd.arguments": [
		"--clang-tidy=true",
		"--suggest-missing-includes=true",
		"--background-index=true",
		"--all-scopes-completion=true",
		"--completion-style=detailed",
		"--header-insertion=iwyu",
		"--header-insertion-decorators",
		"--compile-commands-dir=${workspaceFolder}/build",
		"--pch-storage=memory",
		"--pretty",
		"-j=8"
	],
	"cSpell.words": [
		"buildtool",
		"chrono",
		"moveit",
		"ompl",
		"rclcpp",
		"Ruckig",
		"urdf",
		"waitables",
		"xacro"
	],
	"editor.inlayHints.enabled": "on",
	"editor.inlayHints.fontFamily": "'Jetbrains Mono', 'Cascadia Code', 'Fira Code Retina', Consolas, 'Courier New', monospace",
	"editor.inlayHints.fontSize": 12,
	"editor.inlayHints.padding": true,
	"editor.inlineSuggest.enabled": true,
	"python.analysis.inlayHints.variableTypes": true,
	"python.analysis.inlayHints.functionReturnTypes": true,

	"terminal.integrated.fontFamily": "Hasklug NF",
	"editor.semanticHighlighting.enabled": true,
	"editor.formatOnSave": true,
	"python.analysis.autoImportCompletions": true,
	"python.autoComplete.extraPaths": [
		"/opt/ros/humble/lib/python3.10/site-packages"
	],
	"python.analysis.extraPaths": [
		"/opt/ros/humble/lib/python3.10/site-packages"
	],
	"python.analysis.autoSearchPaths": true,
	"python.analysis.completeFunctionParens": true,
	"python.analysis.useLibraryCodeForTypes": true,
	"python.linting.enabled": true,
	"python.linting.pylintPath": "/usr/local/bin/mypy",
	"python.formatting.provider": "yapf", // or "black" here
	"python.formatting.yapfPath": "/usr/local/bin/yapf",
	"python.linting.pylintEnabled": true,
	"python.linting.mypyEnabled": true,
	"python.linting.lintOnSave": true,
	"python.linting.mypyPath": "/user/local/bin/mypy",
	"vsintellicode.features.python.deepLearning": "enabled",
	"vsintellicode.python.completionsEnabled": true,
	"vsintellicode.sql.completionsEnabled": true,
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"cSpell.enableFiletypes": [
		"xml"
	]
}